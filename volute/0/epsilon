FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 59.744233828;

boundaryField
{

    Inlet
    {
        type            fixedValue;
        value           uniform 59.744233828;        
    }
    
    Outlet
    {
        type            inletOutlet;
        inletValue      uniform 59.744233828;
    }

    "(Wall_1|Wall_2|Wall_3)"
    {
        type            epsilonWallFunction;
        value           $internalField;
    }

    "(AMI1_2|AMI2_2|AMI2_1|AMI1_1)"
    {
        type            cyclicAMI;
    }
}


